          Data="M12 15a3 3 0 1 0 0-6 3 3 0 0 0 0 6zM19.4 15a1.65 1.65 0 0 0 .33 1.82l.06.06a2 2 0 0 1 0 2.83 2 2 0 0 1-2.83 0l-.06-.06a1.65 1.65 0 0 0-1.82-.33 1.65 1.65 0 0 0-1 1.51V21a2 2 0 0 1-2 2 2 2 0 0 1-2-2v-.09A1.65 1.65 0 0 0 9 19.4a1.65 1.65 0 0 0-1.82.33l-.06.06a2 2 0 0 1-2.83 0 2 2 0 0 1 0-2.83l.06-.06a1.65 1.65 0 0 0 .33-1.82 1.65 1.65 0 0 0-1.51-1H3a2 2 0 0 1-2-2 2 2 0 0 1 2-2h.09A1.65 1.65 0 0 0 4.6 9a1.65 1.65 0 0 0-.33-1.82l-.06-.06a2 2 0 0 1 0-2.83 2 2 0 0 1 2.83 0l.06.06a1.65 1.65 0 0 0 1.82.33H9a1.65 1.65 0 0 0 1 1.51V3a2 2 0 0 1 2-2 2 2 0 0 1 2 2v.09a1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82-.33l.06-.06a2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83l-.06.06a1.65 1.65 0 0 0-.33 1.82V9a1.65 1.65 0 0 0 1.51 1H21a2 2 0 0 1 2 2 2 2 0 0 1-2 2h-.09a1.65 1.65 0 0 0-1.51 1z"/>
    
    <!-- File Icon -->
    <Path x:Key="FileIcon" Fill="{DynamicResource PrimaryTextBrush}"
          Data="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8zM14 8V2l6 6z"/>
    
    <!-- Copy Icon -->
    <Path x:Key="CopyIcon" Fill="{DynamicResource AccentTextBrush}" StrokeThickness="2" 
          Data="M16 4h2a2 2 0 0 1 2 2v14a2 2 0 0 1-2 2H6a2 2 0 0 1-2-2V6a2 2 0 0 1 2-2h2m8-2H8a2 2 0 0 0-2 2v2h12V4a2 2 0 0 0-2-2z"/>
    
    <!-- Download Icon -->
    <Path x:Key="DownloadIcon" Fill="{DynamicResource AccentTextBrush}" StrokeThickness="2"
          Data="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4M7 10l5 5 5-5M12 15V3"/>
    
    <!-- Search Icon -->
    <Path x:Key="SearchIcon" Fill="{DynamicResource PrimaryTextBrush}" StrokeThickness="2"
          Data="m21 21-6-6m2-5a7 7 0 1 1-14 0 7 7 0 0 1 14 0z"/>

    <!-- Wedding/Heart Icon -->
    <Path x:Key="WeddingIcon" Fill="#FF69B4" 
          Data="M20.84 4.61a5.5 5.5 0 0 0-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 0 0-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 0 0 0-7.78z"/>

    <!-- Sun Icon (Light Mode) -->
    <Path x:Key="SunIcon" Fill="#FFD60A"
          Data="M12 7a5 5 0 1 1 0 10 5 5 0 0 1 0-10zM12 1v2M12 21v2M4.22 4.22l1.42 1.42M18.36 18.36l1.42 1.42M1 12h2M21 12h2M4.22 19.78l1.42-1.42M18.36 5.64l1.42-1.42"/>

    <!-- Moon Icon (Dark Mode) -->
    <Path x:Key="MoonIcon" Fill="#8E8E93"
          Data="M21 12.79A9 9 0 1 1 11.21 3 7 7 0 0 0 21 12.79z"/>

    <!-- Play/Start Icon -->
    <Path x:Key="PlayIcon" Fill="#34C759"
          Data="M8 5v14l11-7z"/>

    <!-- Pause/Stop Icon -->
    <Path x:Key="StopIcon" Fill="#FF453A"
          Data="M6 4h4v16H6zM14 4h4v16h-4z"/>

</ResourceDictionary>
