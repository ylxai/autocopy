<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- MacOS Style Icons using Path Geometry -->
    
    <!-- Folder Icon -->
    <Path x:Key="FolderIcon" Fill="{DynamicResource AccentTextBrush}" 
          Data="M4 4h4l2 2h6a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V6a2 2 0 0 1 2-2z"/>
    
    <!-- Success/Check Icon -->
    <Path x:Key="CheckIcon" Fill="#34C759" StrokeThickness="2" Stroke="#34C759"
          Data="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"/>
    
    <!-- Warning/Exclamation Icon -->
    <Path x:Key="WarningIcon" Fill="#FF9F0A" 
          Data="M12 2L1 21h22M12 6l7.53 13H4.47M11 10v4h2v-4m-2 6v2h2v-2"/>
    
    <!-- Error/X Icon -->
    <Path x:Key="ErrorIcon" Fill="#FF453A" StrokeThickness="2" Stroke="#FF453A"
          Data="M18 6L6 18M6 6l12 12"/>
    
    <!-- Processing/Sync Icon -->
    <Path x:Key="SyncIcon" Fill="#007AFF" StrokeThickness="2" Stroke="#007AFF"
          Data="M1 4v6h6M23 20v-6h-6M20.49 9A9 9 0 0 0 5.64 5.64L1 10m22 4l-4.64 4.36A9 9 0 0 1 3.51 15"/>
    
